<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.renkaen.cat_hospital.mapper.PermissionMapper">

    <select id="selectAllPermissionId" resultType="Integer">
        SELECT permission_id
        FROM permission_table
    </select>
    <select id="selectAllPermission" resultType="com.renkaen.cat_hospital.bean.DO.Permission">
        select *
        from permission_table
    </select>

</mapper>